<h1><%= "#{t("actions.new")} #{PriceBucketShippingRate.human_name}" %></h1>

<%= error_messages_for :price_bucket_shipping_rate %>

<% form_for(:price_bucket_shipping_rate, :url => collection_url) do |f| %>
